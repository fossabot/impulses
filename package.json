  "description": "Communication Bus",
  "main": "lib/commbus.js",
  "scripts": {
    "build": "webpack --env build",
    "build:watch": "webpack -w --config webpack.config.js",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "test:export": "mkdir -p output-test && mocha --compilers js:babel-register --recursive > output-test/index.txt",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "test:export:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "webpack": "3.1.0",
    "yargs": "7.1.0"
  },
  "repository": {
